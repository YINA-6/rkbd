.btm {
    width: 100%;
    height: 100rpx;
    border-bottom: 5rpx solid #f3f6f8;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.collect {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    text-align: center;
}

.collect text {
    margin-left: 15rpx;
}

.Stide-box {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    text-align: center;
}

.Stide-box text {
    margin-top: 5rpx;
    margin-left: 15rpx;
}

.line-box {
    width: 100%;
    height: 100rpx;
    background-color: #f3f6f8;
}

.circle-box {
    display: flex;
    justify-content: flex-start;
    flex-direction: row;
    flex-wrap: wrap;
    margin-left: 10rpx;
    margin-top: 10rpx;
}

.circle {
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    background-color: #dcf2fc;
    border-radius: 50%;
    height: 100rpx;
    width: 100rpx;
    margin: 10rpx;
}